{
  "name": "react-control-render",
  "version": "1.0.0",
  "description": "Easy-to-use tool to handle the content that needs to be rendered only on the client-side or on the server-side",
  "keywords": [
    "react",
    "rendering",
    "client",
    "server",
    "browser",
    "ssr",
    "server side rendering",
    "csr",
    "client side rendering"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:YelyseiLukin/react-control-render.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/YelyseiLukin/react-control-render#readme",
  "author": "Yelysei Lukin <ylukin277@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@types/jest": "^25.1.0",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.19",
    "jest": "^25.1.0",
    "node-sass": "^4.13.1",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "^25.0.0",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.7.5"
  }
}
